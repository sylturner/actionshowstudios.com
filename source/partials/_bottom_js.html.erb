<%= javascript_include_tag 'jquery.easing.min' %>
<%= javascript_include_tag 'bracket/jquery.bracket.min' %>
<script>
  function clickBracket(data) {
    $("#match-info").html("<h4>"+data+"</h4>");
  }
$(function() {
  if(typeof bracket != 'undefined') {
    $('#bracket').bracket({
      skipConsolationRound: true,
      onMatchClick: clickBracket,
      init: bracket})
  }
});
</script>
